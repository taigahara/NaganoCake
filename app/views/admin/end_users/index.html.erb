<table class="table">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @end_users.each do |end_user| %>
      <tr>
      <td><%= end_user.id %></td>
    	<td>
        <%= end_user.last_name %><%= end_user.first_name %>
      </td>
    	<td><%= end_user.email %></td>
      <td>
      <% unless end_user.paranoia_destroyed? %>
          有効
      <% else %>
          退会済み
      <% end %>
      </td>
      </tr>
    <% end %>
  </tbody>
</table>