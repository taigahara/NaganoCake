<h1>エンドユーザーのマイページ</h1>

<h3>登録情報</h3>
<%= link_to "編集する", edit_end_users_path %>

<table class='table'>
      <tr>
    	  <th>氏名</th>
        <th><%= current_end_user.last_name %></th>
        <th><%= current_end_user.first_name %></th>
      </tr>
      <tr>
    	  <th>カナ</th>
        <th><%= current_end_user.last_name_kana %></th>
        <th><%= current_end_user.first_name_kana %></th>
      </tr>
      <tr>
    	  <th>郵便番号</th>
    	  <th><%= current_end_user.postal_code %></th>
      </tr>
      <tr>
    	  <th>住所</th>
    	  <th><%= current_end_user.address %></th>
      </tr>
      <tr>
    	  <th>電話番号</th>
    	  <th><%= current_end_user.phone_number %></th>
      </tr>
      <tr>
    	  <th>メールアドレス</th>
    	  <th><%= current_end_user.email %></th>
      </tr>
</table>